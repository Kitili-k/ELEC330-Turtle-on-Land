              ixy="0.0" ixz="0.0" iyz="0.0"/>
    </inertial>
    <!--Origin description-->
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
        <!--Filepath to STL visual Mesh file-->
      <geometry>
        <mesh filename="package://turtle/meshes/visual/base_link.STL"/>
      </geometry>
      <material
        name="">
        <!--Link Color-->
        <color 
          rgba="0 1 0 1" />
      </material>
    </visual>
    <!--Collision parameter for Link-->
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
        <!--Filepath to STL visual Mesh file-->
      <geometry>
        <mesh
          filename="package://turtle/meshes/visual/base_link.STL" />
      </geometry>
    </collision>
  </link>

